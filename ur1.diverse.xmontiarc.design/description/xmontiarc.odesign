        <toolSections name="NewSubcomponentCreation" label="New Subcomponent">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="NewPrinter" label="New Printer" nodeMappings="//@ownedViewpoints[name='components']/@ownedRepresentations[name='Component%20Diagram']/@defaultLayer/@containerMappings[name='ComponentTypeContainer']/@subNodeMappings[name='SubcomponentNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="xmontiarc.Subcomponent" referenceName="subcomponents">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="NewPrinter"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="type"/>
                </subModelOperations>
              </firstModelOperations>
